v position=15;

@setup:
    MOV 0 R5;
    MOV 1 R6;
    MOV 1 R8;

@loop:
    JSR @getNextFib;
    MOV ACC R3;
    MOV R8 ACC;
    JNE !position @loop;
    JMP @finish;

@finish:
    HLT;

@getNextFib:
    ADD 1 R8;
    MOV ACC R8;
    ADD R5 R6;
    MOV R6 R5;
    MOV ACC R6;
    PSH ACC;
    RET;
